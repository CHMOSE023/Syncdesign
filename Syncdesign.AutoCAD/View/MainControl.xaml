<UserControl x:Class="Syncdesign.AutoCAD.View.MainControl"           
             xmlns:bim="http://bimhub.top/syncdesign/2026/xaml" 
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Syncdesign.AutoCAD.View"             
             mc:Ignorable="d" 
             Background="{DynamicResource ApplicationBackgroundBrush}"
             d:DesignHeight="450" d:DesignWidth="800"
             >
    <UserControl.Resources>
        <ResourceDictionary>            
            <ResourceDictionary.MergedDictionaries>
                <bim:ControlsDictionary/>
                <bim:ThemesDictionary  /> 
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>             
    </UserControl.Resources>
    <StackPanel >
        <bim:SymbolIcon  Symbol="Settings48"/>
    </StackPanel>
</UserControl>
